{
  "name": "@apify/docusaurus-plugin-typedoc-api",
  "version": "4.2.11-1",
  "description": "Docusaurus plugin that provides source code API documentation powered by TypeDoc. ",
  "keywords": [
    "docusaurus",
    "plugin",
    "typedoc",
    "api"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:apify/docusaurus-plugin-typedoc-api.git"
  },
  "author": "Miles Johnson, Apify Technologies",
  "license": "MIT",
  "main": "src/index.ts",
  "types": "./lib/index.d.ts",
  "files": [
    "assets/**/*",
    "lib/**/*",
    "src/**/*",
    "styles.css"
  ],
  "engines": {
    "node": ">=16.12.0"
  },
  "scripts": {
    "clean": "rimraf ./lib",
    "compile": "tsc -p ./tsconfig.lib.json",
    "build": "npm run clean && tsc --version && npm run compile && npm run copyfiles && npm run fixEntryPoints",
    "copyfiles": "cp -r ./src/plugin/python/docspec-gen ./lib/plugin/python/ && cp ./src/plugin/python/type-parsing/parse_types.py ./lib/plugin/python/type-parsing/parse_types.py",
    "fixEntryPoints": "cp ./package.json ./lib/package.json && sed 's/\"main\": \"src\\/index.ts\"/\"main\": \".\\/index.js\"/g' ./lib/package.json > ./lib/package.json.temp && mv ./lib/package.json.temp ./lib/package.json"
  },
  "packemon": {
    "format": "lib",
    "platform": "node",
    "externals": [
      "@theme/*",
      "@docusaurus/*",
      "@vscode/*",
      "css$"
    ]
  },
  "peerDependencies": {
    "@docusaurus/core": "^3.5.2",
    "@docusaurus/mdx-loader": "^3.5.2",
    "react": ">=18.0.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@docusaurus/plugin-content-docs": "^3.5.2",
    "@docusaurus/types": "^3.5.2",
    "@docusaurus/utils": "^3.5.2",
    "@types/react": "^18.3.11",
    "@vscode/codicons": "^0.0.35",
    "html-entities": "2.3.2",
    "marked": "^9.1.6",
    "marked-smartypants": "^1.1.5",
    "typedoc": "^0.26.11",
    "zx": "^8.1.4"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^3.5.2",
    "@docusaurus/tsconfig": "^3.5.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^6.0.1",
    "typescript": "^5.3.3"
  },
  "stableVersion": "4.2.10"
}
