{
  "name": "@apify/docusaurus-plugin-typedoc-api",
  "version": "4.4.8",
  "description": "Docusaurus plugin that provides source code API documentation powered by TypeDoc. ",
  "keywords": [
    "docusaurus",
    "plugin",
    "typedoc",
    "api"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:apify/docusaurus-plugin-typedoc-api.git"
  },
  "author": "Miles Johnson, Apify Technologies",
  "license": "MIT",
  "main": "src/index.ts",
  "types": "./lib/index.d.ts",
  "files": [
    "assets/**/*",
    "lib/**/*",
    "python-scripts/**/*",
    "src/**/*",
    "styles.css"
  ],
  "engines": {
    "node": ">=16.12.0"
  },
  "packemon": {
    "format": "lib",
    "platform": "node",
    "externals": [
      "@theme/*",
      "@docusaurus/*",
      "@vscode/*",
      "css$",
      "cheerio*"
    ]
  },
  "peerDependencies": {
    "@docusaurus/core": "^3.8.1",
    "@docusaurus/mdx-loader": "^3.8.1",
    "@docusaurus/plugin-content-docs": "^3.8.1",
    "@docusaurus/preset-classic": "^3.8.1",
    "@docusaurus/types": "^3.8.1",
    "@docusaurus/utils": "^3.8.1",
    "@types/react": "^18.3.11 || >=19.0.0",
    "react": ">=18.0.0 || >=19.0.0",
    "react-dom": "^18.2.0 || >=19.0.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@vscode/codicons": "^0.0.35",
    "html-entities": "2.3.2",
    "marked": "^9.1.6",
    "marked-smartypants": "^1.1.5",
    "typedoc": "^0.26.11",
    "zx": "^8.1.4"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^3.8.1",
    "@docusaurus/plugin-content-docs": "^3.8.1",
    "@docusaurus/tsconfig": "^3.8.1",
    "@docusaurus/types": "^3.8.1",
    "@docusaurus/utils": "^3.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.3.3"
  }
}
